{
  "$schema": "https://json-schema.org/draft-07/schema",
  "title": "Generate pages during the build",
  "oneOf": [
    {
      "markdownDescription": "https://github.com/oprypin/mkdocs-gen-files",
      "enum": [
        "git-authors"
      ]
    },
    {
      "type": "object",
      "properties": {
        "gen-files": {
          "additionalProperties": false,
          "properties": {
            "scripts": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          },
          "type": ["object", "null"]
        }
      }
    }
  ]
}
